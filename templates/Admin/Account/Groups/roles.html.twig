{# Template Parts #}
{% extends "Admin/base.html.twig" %}

{# Document #}
{% block head_title %}{{ title('account_group_role_title'|trans) }}{% endblock %}

{# Content Title #}
{% block content_head %}
    <a href="{{ app.session.get('backUrl') }}" class="back-icon"><i class="material-icons">arrow_back</i></a>
    <span class="text">{{ 'account_group_role_title'|trans }}</span>
    <span class="desc">{{ group.name }}</span>
{% endblock %}

{# Content Navigation #}
{% block content_nav %}{{ pd_menu_render('App\\Menu\\GroupsMenu', { 'group': group.id }) }}{% endblock %}

{% use "Admin/Account/rolesTemplate.html.twig" %}
