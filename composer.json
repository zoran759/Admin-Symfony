{
  "name": "appaydin/pd-admin",
  "description": "Symfony Powerful Admin Dashboard",
  "homepage": "https://github.com/appaydin/pd-admin",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Kerem APAYDIN",
      "homepage": "http://apaydin.me"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "appaydin/pd-mailer": "^1.1",
    "appaydin/pd-menu": "^1.0",
    "appaydin/pd-user": "^1.11",
    "appaydin/pd-widget": "^1.0",
    "beberlei/DoctrineExtensions": "^1.0",
    "intervention/image": "^2.4",
    "knplabs/knp-paginator-bundle": "^2.8",
    "sensio/framework-extra-bundle": "^5.2",
    "symfony/asset": "4.*",
    "symfony/console": "4.*",
    "symfony/dotenv": "4.*",
    "symfony/expression-language": "4.*",
    "symfony/flex": "1.*",
    "symfony/form": "4.*",
    "symfony/framework-bundle": "4.*",
    "symfony/monolog-bundle": "3.*",
    "symfony/orm-pack": "1.*",
    "symfony/security-bundle": "4.*",
    "symfony/swiftmailer-bundle": "3.*",
    "symfony/translation": "4.*",
    "symfony/twig-bundle": "4.*",
    "symfony/validator": "4.*",
    "symfony/yaml": "4.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/debug-pack": "1.*",
    "symfony/maker-bundle": "1.*",
    "symfony/test-pack": "1.*",
    "symfony/web-server-bundle": "4.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.2.*"
    }
  }
}
