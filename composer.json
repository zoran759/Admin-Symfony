{
  "name": "appaydin/pd-admin",
  "description": "Symfony Powerful Admin Dashboard",
  "homepage": "https://github.com/appaydin/pd-admin",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Kerem APAYDIN",
      "homepage": "http://apaydin.me"
    }
  ],
  "require": {
    "php": "^7.2.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "appaydin/pd-mailer": "^1.2",
    "appaydin/pd-menu": "^1.2",
    "appaydin/pd-user": "^1.20",
    "appaydin/pd-widget": "^1.2",
    "beberlei/doctrineextensions": "^1.0",
    "intervention/image": "^2.5",
    "knplabs/knp-paginator-bundle": "^4.0",
    "sensio/framework-extra-bundle": "^5.3",
    "symfony/asset": "4.3.*",
    "symfony/console": "4.3.*",
    "symfony/dotenv": "4.3.*",
    "symfony/expression-language": "4.3.*",
    "symfony/flex": "1.*",
    "symfony/form": "4.3.*",
    "symfony/framework-bundle": "4.3.*",
    "symfony/monolog-bundle": "^3.4",
    "symfony/orm-pack": "1.*",
    "symfony/process": "4.3.*",
    "symfony/security-bundle": "4.*",
    "symfony/swiftmailer-bundle": "3.2.*",
    "symfony/translation": "4.3.*",
    "symfony/twig-bundle": "4.3.*",
    "symfony/validator": "4.3.*",
    "symfony/yaml": "4.3.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/debug-bundle": "^4.3",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/test-pack": "1.*",
    "symfony/var-dumper": "4.3.*",
    "symfony/web-server-bundle": "4.3.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.3.*"
    }
  }
}
